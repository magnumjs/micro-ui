{
  "name": "@magnumjs/micro-ui",
  "version": "1.2.0",
  "description": "A tiny reactive component system with declarative slots, keyed lists, reactive props, and DOM diffing.",
  "type": "module",
  "main": "dist/magnumjs-micro-ui.js",
  "unpkg": "dist/magnumjs-micro-ui-all.js",
  "module": "dist/magnumjs-micro-ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/magnumjs-micro-ui.esm.js",
      "require": "./dist/magnumjs-micro-ui.js"
    },
    "./esm": {
      "default": "./dist/magnumjs-micro-ui.esm.js",
      "import": "./dist/magnumjs-micro-ui.esm.js",
      "require": "./dist/magnumjs-micro-ui.js"
    },
    "./context": {
      "import": "./dist/magnumjs-micro-ui-context.esm.js",
      "require": "./dist/magnumjs-micro-ui-context.js"
    },
    "./hooks": {
      "import": "./dist/magnumjs-micro-ui-hooks.esm.js",
      "require": "./dist/magnumjs-micro-ui-hooks.js"
    },
    "./compose": {
      "import": "./dist/magnumjs-micro-ui-compose.esm.js",
      "require": "./dist/magnumjs-micro-ui-compose.js"
    }
  },
  "browser": "dist/magnumjs-micro-ui.js",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "bump": "npm version patch --no-git-tag-version",
    "build": "node build.js",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "start": "node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --coverageReporters=text --coverageReporters=html --coverageDirectory=coverage"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "esbuild": "^0.19.12",
    "express": "^4.18.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "standard-version": "^9.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magnumjs/micro-ui.git"
  },
  "keywords": [
    "reactive",
    "framework",
    "minimal",
    "vanilla-js",
    "js",
    "library",
    "component",
    "template",
    "architecture",
    "modular"
  ],
  "author": "Michael Glazer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magnumjs/micro-ui/issues"
  },
  "homepage": "https://github.com/magnumjs/micro-ui#readme"
}
